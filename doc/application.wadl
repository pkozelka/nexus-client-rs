  <wadl:resources base="http://localhost:8080/nexus-staging-plugin">
    <wadl:resource path="/service/local/staging/bundle_upload">
      <wadl:method name="PUT">
        <wadl:doc>
          <![CDATA[Handles the uploaded bundle files (multiple of them).]]>
        </wadl:doc>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Handles the uploaded bundle files (multiple of them).]]>
        </wadl:doc>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/service/local/staging/profile_evaluate">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Returns information about corresponding profile matching the given repository type and Maven GAV.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="t" style="query">
            <wadl:doc>
              <![CDATA[- repository type. One of "maven1" or "maven2"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="g" style="query">
            <wadl:doc>
              <![CDATA[- group id]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="a" style="query">
            <wadl:doc>
              <![CDATA[- artifact id]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="v" style="query">
            <wadl:doc>
              <![CDATA[- version]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="m" style="query">
            <wadl:doc>
              <![CDATA[- (optional) staging mode. One of "DEPLOY", "UPLOAD", "BOTH" or "GROUP". Defaults to "DEPLOY"]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[list of staging profiles matching the repository type/ GAV]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="stagingProfiles"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
